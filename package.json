{
  "name": "shuten",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "start:exp": "TAMAGUI_OPTIMIZE_NATIVE_VIEWS=1 TAMAGUI_ENABLE_DYNAMIC_LOAD=1 yarn start",
    "start:prod": "DISABLE_EXTRACTION=false TAMAGUI_OPTIMIZE_NATIVE_VIEWS=1 TAMAGUI_ENABLE_DYNAMIC_LOAD=1 expo start --dev-client --offline --no-dev --minify",
    "android": "APP_VARIANT=development yarn expo run:android",
    "ios": "APP_VARIANT=development yarn expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "upgrade:tamagui": "yarn upgrade --latest --pattern '*tamagui*' ; npx syncyarnlock --save --keepPrefix ; prettier --write package.json"
  },
  "jest": {
    "preset": "jest-expo"
  },
