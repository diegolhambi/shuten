{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "isolatedModules": true,
        "moduleDetection": "force"
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts"
    ]
}
